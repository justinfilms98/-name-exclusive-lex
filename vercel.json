{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/auth/(.*)", "dest": "/api/auth/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 